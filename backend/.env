# 服务器配置
PORT=3001
NODE_ENV=development

# 前端地址（生产环境）
FRONTEND_URL=http://localhost:3000

# 智能体配置文件路径
AGENTS_CONFIG_PATH=../config/agents.json

# 日志级别
LOG_LEVEL=debug

# 请求超时时间（毫秒）
REQUEST_TIMEOUT=30000

# 速率限制配置
RATE_LIMIT_POINTS=100
RATE_LIMIT_DURATION=60
RATE_LIMIT_BLOCK_DURATION=60

# 豆包 SeeDream 图片生成配置
DOUBAO_API_KEY=ca7d9d5f-8c9a-4e1d-aa9e-4f2d1a86262c
DOUBAO_IMAGE_MODEL=doubao-seedream-4-0-250828
# 可选：生成尺寸（如需4:3可设置为 1728x2304）
DOUBAO_IMAGE_SIZE=1024x1024
# 可选：是否启用流式返回（默认false），开启后端需处理SSE/流式响应
DOUBAO_IMAGE_STREAM=false